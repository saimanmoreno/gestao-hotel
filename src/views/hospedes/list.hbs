<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row bg-title">
            <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                <h4 class="page-title">{{ title }}</h4>
            </div>
            <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
                <ol class="breadcrumb">
                    <li><a href="#">{{ title }}</a></li>
                    <li class="active">Lista</li>
                </ol>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="white-box">
                    <div class="container mt-5">
                        <div class="row">
                            <div class="col-md-10">
                                <table class="table table-bordered table-hover text-center">
                                    <thead>
                                        <tr>
                                            <td>NIF</td>
                                            <td>Nome</td>
                                            <td>Nacionalidade</td>
                                            <td>Email</td>
                                            <td>Telefone</td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each hospedes}}
                                        <tr>
                                            <td>1111111{{ nif }}</td>
                                            <td>{{ nome_h }}</td>
                                            <td>{{ nacionalidade }}</td>
                                            <td>{{ email }}</td>
                                            <td>{{ telefone }}</td>
                                        </tr>
                                        {{else}}
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /.container-fluid -->
    <footer class="footer text-center"> 2020 &copy; Gestão de Projetos Informáticos </footer>
</div>
<!-- ============================================================== -->
<!-- End Page Content -->
<!-- ============================================================== -->